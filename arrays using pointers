

#include <iostream>

using namespace std;

int main()
{
    int a[10];
    int *k;
    
    cout<<"Enter elements\n";
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    k = a;
    cout<<"You entered : \n";
    
    for(int i=0;i<5;i++)
    {
        cout<<*k<<endl;
        k++;
    }
    
    
    return 0;
}
